class Impy : DoomImp replaces DoomImp {
Default {
  }
  
	States {
		Death:
			TROO I 8 ACS_NamedExecute("shtgcd");
			TROO J 8 A_Scream();
			TROO K 6;
			TROO L 6 A_NoBlocking();
			TROO M -1;
			stop;
		  XDeath:
			TROO N 5 ACS_NamedExecute("shtgcd");
			TROO O 5 A_XScream();
			TROO P 5;
			TROO Q 5 A_NoBlocking();
			TROO RST 5;
			TROO U -1;
			stop;
	}
}